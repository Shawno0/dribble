{
  "name": "@dribble/compiler",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "bin": {
    "dribblec": "dist/cli.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm,cjs --sourcemap",
    "typecheck": "tsc -p tsconfig.json --noEmit",
  "prepare": "npm run build",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0",
    "eslint": "^9.9.0",
    "@typescript-eslint/parser": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "eslint-plugin-import": "^2.29.1",
  "prettier": "^3.3.3",
  "@types/node": "^20.14.12"
  }
}
